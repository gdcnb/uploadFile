bgimgconf(url)
  background-image url(url)
  background-repeat no-repeat

vendor(prop, args)
  -webkit-{prop} args
  -moz-{prop} args
  -o-{prop} args
  {prop} args

border-radius()
  vendor('border-radius', arguments)

flex()
  vendor('flex', arguments)

box-shadow()
  vendor('box-shadow', arguments)

transition()
    vendor('transition', arguments)

transform()
    vendor('transform', arguments)


    
